<%- include("partials/header")%>
        <h1><%= name %></h1>
        <% for(let post of posts){ %>
            <h2> <%= post.author %></h2>
            <% if(post.img){ %>
                <img src="<%= post.img %>" alt="">
            <% } %>
            <p> <%= post.title %> </p>
        <% } %>
<%- include("partials/footer")%>